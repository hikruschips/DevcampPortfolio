<div id="<%= dom_id portfolio %>">
  
  <%= image_tag portfolio.thumb_image unless portfolio.thumb_image.nil? %>

  <p>
    <strong>Title:</strong>
    <%= portfolio.title %>
  </p>
  <p>
    <strong>Subtitle:</strong>
    <%= portfolio.subtitle %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= portfolio.body %>
  </p>

</div>
